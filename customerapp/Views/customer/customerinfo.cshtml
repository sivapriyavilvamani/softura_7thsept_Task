@model IEnumerable<customerapp.model.customerClass>

@{
    Layout = "_layout";
}
<table class=" table">
    <thead>

        <tr>
            <th>@Html.DisplayNameFor(model => model.Id)</th>
            <th>@Html.DisplayNameFor(model => model.Name)</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var cust in Model)
        {
            <tr>
                <td>@Html.DisplayNameFor(modelitem => cust.Id)</td>
                <td>@cust.Name</td>
               
            </tr>
        }
    </tbody>
</table>

